#include <iostream>
using namespace std;


int main()
{
    int value, p=0;
    int arr[5];
    for (int i = 0; i < 5; i++) {
        cout << "enter numbers: ";
        cin >> arr[i];

    }
    cout << "enter value you want to find: ";
    cin >> value;
    for (int i = 0; i < 5; i++) {
        if (arr[i] == value)
            p = i + 1;
    }
    if (p == 0)
        cout << "number not found..";
    else
    cout << "number is at position  " << p;
    
}
